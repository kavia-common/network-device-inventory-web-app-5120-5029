{
  "name": "network-device-inventory-react-frontend",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "prestart": "node -e \"process.exit(0)\"",
    "start": "cross-env PORT=${PORT:-3000} VITE_PORT=${VITE_PORT:-$PORT} vite --host",
    "dev": "cross-env PORT=${PORT:-3000} VITE_PORT=${VITE_PORT:-$PORT} vite --host",
    "start:preview": "npm run build && cross-env PORT=${PORT:-3000} VITE_PORT=${VITE_PORT:-$PORT} vite preview --host",
    "start:3002": "cross-env PORT=3002 VITE_PORT=3002 vite --host",
    "build": "tsc -b && vite build",
    "preview": "cross-env PORT=${PORT:-3000} VITE_PORT=${VITE_PORT:-$PORT} vite preview --host",
    "lint": "eslint .",
    "format": "prettier --write .",
    "test": "echo \"No tests yet\" && exit 0"
  },
  "dependencies": {
    "axios": "^1.7.7",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.26.2"
  },
  "devDependencies": {
    "@types/node": "^22.7.5",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.8.1",
    "@typescript-eslint/parser": "^8.8.1",
    "@vitejs/plugin-react": "^4.3.1",
    "eslint": "^9.12.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "prettier": "^3.3.3",
    "typescript": "^5.6.3",
    "vite": "^5.4.8",
    "cross-env": "^7.0.3"
  },
  "engines": {
    "node": ">=18.18.0"
  }
}
